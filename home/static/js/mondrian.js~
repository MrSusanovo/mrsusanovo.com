var mondApp = angular.module('mondrian',['ngAnimate']);
mondApp.controller('mondCtrl', function($scope){
    $scope.visible0 = false;
    $scope.visible1 = false;
    $scope.visible2 = false;
    $scope.visible3 = false;
    $scope.visible4 = false;
    $scope.visible5 = false;
    $scope.visible6 = false;
    $scope.visible7 = false;
});
